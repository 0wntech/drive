{
  "name": "solid-files",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@sentry/browser": "^5.15.5",
    "babel-plugin-remove-test-ids": "^1.1.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-saver": "^2.0.2",
    "mime": "^2.4.5",
    "node-sass": "^4.14.1",
    "ownfiles": "^1.2.16",
    "ownuser": "^1.3.11",
    "rdflib": "^1.2.1",
    "react": "^16.13.1",
    "react-collapse": "^5.0.1",
    "react-contexify": "^4.1.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0",
    "react-spinners-kit": "^1.9.1",
    "react-test-renderer": "^16.13.1",
    "react-textarea-autosize": "^8.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "solid-auth-client": "^2.4.1",
    "solid-namespace": "^0.3.0",
    "string-similarity": "^4.0.1",
    "styled-components": "^5.1.0",
    "url": "^0.11.0",
    "your-user": "^1.1.12"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:unit": "cross-env CI=true npm test --env=jsdom",
    "test": "cross-env CI=true react-scripts test",
    "test:e2e": "rimraf ./user-data && jest -c ./test/e2e_tests/login.jest.config.json && jest -c ./test/e2e_tests/jest.config.json",
    "test:watch": "npm test --watchAll",
    "test:all": "npm run test:unit --coverage && npm run test:e2e",
    "eject": "react-scripts eject",
    "lint": "eslint src --fix",
    "svgr": "svgr -d src/assets/svgIcons src/assets/icons"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:unit",
      "pre-push": "lint-staged && npm run test:unit"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/cli": "^5.4.0",
    "codecov": "^3.6.5",
    "dotenv": "^8.2.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "faker": "^4.1.0",
    "fetch-mock": "^9.4.0",
    "husky": "^4.2.5",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "puppeteer": "^3.0.2",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.4.2"
  }
}
