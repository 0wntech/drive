{
  "name": "solid-files",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-jest": "^24.9.0",
    "babel-plugin-remove-test-ids": "^1.1.0",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "file-saver": "^2.0.2",
    "mime": "^2.4.4",
    "ownfiles": "^1.1.8",
    "ownuser": "^1.3.2",
    "rdflib": "^0.20.1",
    "react": "^16.8.6",
    "react-collapse": "^4.0.3",
    "react-contexify": "^4.1.1",
    "react-dom": "^16.8.6",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.5",
    "react-spinners-kit": "^1.9.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "solid-auth-client": "^2.3.1",
    "solid-namespace": "^0.2.0",
    "styled-components": "^4.4.0",
    "url": "^0.11.0",
    "your-user": "^1.1.11"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:local": "react-scripts test",
    "test:coverage": "npm run test:unit --coverage",
    "test:unit": "cross-env CI=true npm test --env=jsdom -- --testPathIgnorePatterns=e2e",
    "test": "react-scripts test",
    "test:all": "npm run test:unit --coverage && npm run test:e2e",
    "test:e2e": "rimraf ./user-data && npm test -- --testPathPattern=e2e --runInBand",
    "test:watch": "react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint src --fix",
    "svgr": "svgr -d src/assets/svgIcons src/assets/icons"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:unit",
      "pre-push": "lint-staged && npm run test:unit"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{json,css}": [
        "prettier --write",
        "git add"
      ]
    },
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/cli": "^4.3.3",
    "codecov": "^3.6.1",
    "dotenv": "^8.2.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "faker": "^4.1.0",
    "fetch-mock": "^7.5.1",
    "husky": "^2.7.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "puppeteer": "^2.0.0",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.0",
    "source-map-explorer": "^2.1.1"
  }
}
