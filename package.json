    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "flare-react": "^1.0.2",
    "mime": "^2.4.4",
    "ownuser": "^1.2.5",
    "rdflib": "^0.20.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-collapse": "^4.0.3",
    "react-contexify": "^4.1.1",
    "react-dom": "^16.8.6",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.5",
    "react-spinners-kit": "^1.9.0",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "solid-auth-client": "^2.3.1",
    "solid-namespace": "^0.2.0",
    "styled-components": "^4.4.0",
    "url": "^0.11.0",
    "your-acl": "^1.0.17",
    "your-user": "^1.1.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "svgr": "svgr -d src/assets/svgIcons src/assets/icons"
  },
  "husky": {
    "hooks": {
      "pre-commit": "set CI=true && react-scripts test && lint-staged",
      "pre-push": "set CI=true && react-scripts test && lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*/**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ],
      "*.{json,css}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/cli": "^4.3.3",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0"
  }
}
